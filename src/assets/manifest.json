{
  "manifest_version": 3,
  "name": "Tiles",
  "version": "0.22.00",
  "default_locale": "en",
  "description": "Integrated browsing resource manager. Change to your favorite colors to change your mood at work!",
  "permissions": [
    "storage",
    "bookmarks",
    "tabs",
    "history",
    "favicon",
    "sessions"
  ],
  "action": {
    "default_title": "Tiles",
    "default_icon": {
      "48": "t48.png",
      "128": "t128.png"
    }
  },
  "icons": {
    "48": "t48.png",
    "128": "t128.png"
  },
  "commands": {
    "_execute_action": {
      "description": "Tiles",
      "suggested_key": {
        "default": "Alt+P"
      }
    }
  },
  "background": {
    "service_worker": "sw.js"
  },
  "options_page": "settings.html"
}