{
  "manifest_version": 3,
  "name": "Popup'n My Resources",
  "version": "0.18.4",
  "default_locale": "en",
  "description": "Search/view across bookmarks, tabs, and history for a one-stop access to your browsing resources.",
  "permissions": [
    "storage",
    "bookmarks",
    "tabs",
    "history",
    "favicon"
  ],
  "action": {
    "default_title": "Popup'n My Resources",
    "default_icon": {
      "48": "p48.png",
      "128": "p128.png"
    }
  },
  "icons": {
    "48": "p48.png",
    "128": "p128.png"
  },
  "commands": {
    "_execute_action": {
      "description": "Toggle Popup'n My Resources",
      "suggested_key": {
        "mac": "Command+Shift+P",
        "default": "Alt+Shift+P"
      }
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "settings.html"
}